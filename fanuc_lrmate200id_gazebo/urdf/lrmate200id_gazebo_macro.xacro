<?xml version = "1.0" ?>
<robot name = "fanuc_lrmate200id" xmlns:xacro="http://www.ros.org/wiki/xacro" >
<xacro:macro name="fanuc_lrmate200id" params="prefix">


<!--Gazebo ROS Control -->
<gazebo>
  <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>/${prefix}</robotNamespace>
    <legacyModeNS>true</legacyModeNS>
  </plugin>
</gazebo>

<!--Base -->


<link name="${prefix}base_link">
	<collision>
		<geometry>
			<mesh filename = "package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/base_link.stl"  />
		</geometry>
	<origin xyz = "0 0 0" rpy = "0 0 0"/>
	</collision>

	<visual>
 		<geometry>
			<mesh filename = "package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/base_link.stl" />
		</geometry>
	<origin xyz = "0 0 0" rpy = "0 0 0"/>
	</visual>
	<inertial>
		<origin xyz = "0.0 0.0 0.0" rpy = "0 0 0" /> 
		<mass value = "6.215"/>
        <inertia ixx="0.0247272" ixy="-8.0784E-05" ixz="0.00130902" iyy="0.0491285" iyz="-8.0419E-06" izz="0.0472376"/>	</inertial>
</link>
<link name = "world"/>

<joint name="world_base_joint" type="fixed">
	<parent link="world"/>
	<child link="${prefix}base_link" />
	<origin xyz = "0 0 0" rpy = "0 0 0"/>
</joint> 

<gazebo reference= "${prefix}base_link" >
	<material>Gazebo/Black</material>
</gazebo>

<joint name="${prefix}joint_1" type="revolute">
      <origin xyz="0 0 0.330" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}link_1" />
      <axis xyz="0 0 1" />
      <limit effort="0" lower="-2.965" upper="2.965" velocity="7.85" />
      <dynamics damping="1.0" friction="1.0"/>

</joint>

 <link name ="${prefix}link_1">
	<collision>
	<geometry>
		<mesh filename= "package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/link_1.stl"/>
	</geometry>
		<origin xyz="0 0 0" rpy ="0 0 0"/>
	</collision>
	<visual>
	<geometry>
		<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/link_1.stl"/>
	</geometry>
		<origin xyz="0 0 0" rpy="0 0 0"/>
	</visual>
	<inertial>
        <origin xyz="-0.000010 -0.054550 0.028725" rpy="0 0 0"/>
		<mass value = "2.13"/>
        <inertia
          ixx="${600 * 0.000048}" ixy="0.0" ixz="${600 * -0.000006}"
          iyy="${600 * 0.000035}" iyz="0.0" izz="${600 * 0.000037}"/>	</inertial>
</link>

<gazebo reference= "${prefix}link_1" >
	<material>Gazebo/Orange</material>
</gazebo>

<joint name="${prefix}joint_2" type="revolute">
      <origin xyz="0.050 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_1" />
      <child link="${prefix}link_2" />
      <axis xyz="0 1 0" />
      <limit effort="0" lower="-1.745329" upper="2.530727" velocity="6.63" />
      <dynamics damping="1.0" friction="1.0"/>

</joint>

<!--Part3-->
<link name = "${prefix}link_2">
	<collision>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/link_2.stl" />
		</geometry>
	</collision>
	<visual>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry>
			<mesh filename = "package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/link_2.stl" />
		</geometry>
	</visual>
	<inertial>
        <origin xyz="-0.002701 -0.000977 0.159466" rpy="0 0 0"/>
		<mass value = "7.65"/>
        <inertia
          ixx="${600 * 0.000182}" ixy="0.0" ixz="0.0"
          iyy="${600 * 0.000157}" iyz="${600 * -0.000001}"
          izz="${600 * 0.000049}"/>	</inertial>
</link>
<gazebo reference= "${prefix}link_2" >
	<material>Gazebo/Orange</material>
</gazebo>

<joint name="${prefix}joint_3" type="revolute">
      <origin xyz="0 0 0.330" rpy="0 0 0"/>
      <parent link="${prefix}link_2" />
      <child link="${prefix}link_3" />
      <axis xyz="0 -1 0" />
      <limit effort="0" lower="-2.450966" upper="4.886922" velocity="9.08" />
      <dynamics damping="1.0" friction="1.0"/>

</joint>

<link name="${prefix}link_3">
	<collision>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/link_3.stl"/>
		</geometry>
