	<visual>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/link_3.stl"/>
		</geometry>
	</visual>
	<inertial>
        <origin xyz="-0.000065 0.017074 0.008866" rpy="0 0 0"/>
		<mass value = "2.91"/>
        <inertia
          ixx="${600 * 0.000007}" ixy="0.0" ixz="${600 * -0.000001}"
          iyy="${600 * 0.000008}" iyz="0.0"
          izz="${600 * 0.000008}"/>	</inertial>
</link>

<gazebo reference= "${prefix}link_3" >
	<material>Gazebo/Orange</material>
</gazebo>

<joint name="${prefix}joint_4" type="revolute">
      <origin xyz="0 0 0.035" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="-3.315" upper="3.315" velocity="9.60" />
      <dynamics damping="1.0" friction="1.0"/>

</joint>

<link name="${prefix}link_4">
	<collision>
		<origin xyz ="0 0 0" rpy = "0 0 0" />
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/link_4.stl" />
		</geometry>
	</collision>
	<visual>
		<origin xyz ="0 0 0" rpy = "0 0 0" />
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/link_4.stl"/>
		</geometry>
	</visual>
	<inertial>
        <origin xyz="0.000067 0.002203 0.221634" rpy="0 0 0"/>
		<mass value = "3.70"/>
        <inertia
          ixx="${600 * 0.000005}" ixy="0.0" ixz="0.0"
          iyy="${600 * 0.000019}" iyz="0.0"
          izz="${600 * 0.000019}"/>	</inertial>	

</link>

<gazebo reference= "${prefix}link_4" >
	<material>Gazebo/Orange</material>
</gazebo>

<joint name="${prefix}joint_5" type="revolute">
      <origin xyz="0.335 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 -1 0"/>
      <limit effort="0" lower="-2.18" upper="2.18" velocity="9.51" />
	  <dynamics damping="1.0" friction="1.0"/>

</joint>


<link name="${prefix}link_5">
	<collision>
		<origin xyz="0 0 0" rpy ="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/link_5.stl"/>
		</geometry>
	</collision>
	<visual>
		<origin xyz="0 0 0" rpy ="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/link_5.stl"/>
		</geometry>
	</visual>
	<inertial>
        <origin xyz="-0.000010 -0.054550 0.028725" rpy="0 0 0"/>
		<mass value = "1.70"/>
        <inertia
          ixx="${600 * 0.00004}" ixy="0.0" ixz="${600 * -0.000004}"
          iyy="${600 * 0.00003}" iyz="0.0" izz="${600 * 0.00003}"/>	</inertial>	

</link>

<gazebo reference= "${prefix}link_5" >
	<material>Gazebo/Orange</material>
</gazebo>

<joint name="${prefix}joint_6" type="revolute">
      <origin xyz="0.080 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="-6.285" upper="6.285" velocity="17.45" />
	  <dynamics damping="1.0" friction="1.0"/>

</joint>


<link name="${prefix}link_6">
	<collision>
		<origin xyz="0 0 0" rpy ="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/link_6.stl"/>
		</geometry>
	</collision>
	<visual>
		<origin xyz="0 0 0" rpy ="0 0 0"/>
		<geometry>
			<mesh filename="package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/link_6.stl"/>
		</geometry>
	</visual>
	<inertial>
        <origin xyz="-0.000000 0.000000 -0.003006" rpy="0 0 0" />
		<mass value = "0.137"/>
        <inertia
          ixx="0.01" ixy="0.0" ixz="0.0"
          iyy="0.01" iyz="0.0"
          izz="0.01"/>	
	</inertial>	

</link>

<gazebo reference= "${prefix}link_6" >
	<material>Gazebo/Orange</material>
</gazebo>


    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
<link name="${prefix}tool0" />
    <joint name="${prefix}link_6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${pi} ${-pi/2.0} 0" />
      <parent link="${prefix}link_6" />
      <child link="${prefix}tool0" />
</joint>



<!--Transmission-->
 <transmission name="${prefix}tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${prefix}joint_1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="${prefix}joint_1_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="${prefix}tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${prefix}joint_2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="${prefix}joint_2_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

 <transmission name="${prefix}tran3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${prefix}joint_3">
